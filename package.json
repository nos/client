{
  "name": "nOS",
  "description": "Discover & build the web of the future.",
  "repository": "https://www.github.com/nos/client",
  "author": "nOS",
  "homepage": "https://nos.io",
  "version": "0.5.3",
  "private": true,
  "main": "dist/main/main.js",
  "license": "MIT",
  "engines": {
    "node": "^8.10.0 || >=9.10.0"
  },
  "dependencies": {
    "@cityofzion/neon-js": "3.11.9",
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-regular-svg-icons": "5.8.1",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@ledgerhq/hw-transport-node-hid": "4.18.0",
    "@nosplatform/react-copy-to-clipboard": "5.0.1",
    "about-window": "1.12.1",
    "babel-polyfill": "6.26.0",
    "bignumber.js": "8.1.1",
    "bip32": "1.0.2",
    "bip39": "2.5.0",
    "classnames": "2.2.6",
    "coingecko-api": "1.0.4",
    "connected-react-router": "5.0.1",
    "ejs": "2.6.1",
    "electron-context-menu": "0.11.0",
    "electron-is-dev": "0.3.0",
    "electron-json-storage": "4.1.6",
    "electron-localshortcut": "3.1.0",
    "electron-updater": "3.2.3",
    "es6-promisify": "6.0.1",
    "ethereumjs-utils": "5.2.5",
    "file-type": "10.11.0",
    "format-currency": "1.0.0",
    "history": "4.9.0",
    "lodash": "4.17.11",
    "mime-types": "2.1.24",
    "node-fetch": "2.3.0",
    "prop-types": "15.7.2",
    "qrcode.react": "0.9.3",
    "rc-tooltip": "3.7.3",
    "react": "16.8.6",
    "react-click-outside": "3.0.1",
    "react-dom": "16.8.6",
    "react-is": "16.8.6",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-scroll": "1.7.11",
    "react-sticky": "6.0.3",
    "recharts": "1.5.0",
    "recompose": "0.30.0",
    "redux": "3.7.2",
    "redux-saga": "0.16.2",
    "redux-thunk": "2.3.0",
    "sifter": "0.5.3",
    "source-map-support": "0.5.12",
    "spunky": "1.3.1",
    "switch-tree": "0.2.2",
    "uuid": "3.3.2",
    "what-input": "5.2.1",
    "whatwg-url": "7.0.0"
  },
  "scripts": {
    "start": "yarn build:main && run-p -l -n -r start:renderer start:electron",
    "start:renderer": "webpack-dev-server --config config/webpack.renderer.js",
    "start:electron": "wait-on http://localhost:9080/ && electron .",
    "build": "yarn build:main && yarn build:renderer",
    "build:main": "electron-webpack main",
    "build:renderer": "cross-env NODE_ENV=production webpack --config config/webpack.renderer.js",
    "dist": "yarn run clean && yarn run build && cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true electron-builder",
    "pack": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "pretest": "yarn run lint && yarn run stylelint",
    "test": "jest --maxWorkers=4",
    "test:debug": "node inspect ./node_modules/.bin/jest --runInBand",
    "stylelint": "stylelint src/**/*.scss",
    "lint": "eslint --env browser,node,server --ext .jsx,.js --color .",
    "lint:fix": "yarn run lint --fix",
    "clean": "rimraf build dist coverage",
    "postinstall": "electron-builder install-app-deps",
    "release": "yarn run clean && yarn run build && build  --win --x64 --ia32 --linux -p always"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-classes": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-react": "6.24.1",
    "codecov": "3.3.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "cssnano": "4.1.8",
    "electron": "2.0.18",
    "electron-builder": "20.32.0",
    "electron-devtools-installer": "2.2.4",
    "electron-webpack": "1.13.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-canvas-mock": "2.0.0",
    "nock": "10.0.6",
    "node-loader": "0.6.0",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "4.8.4",
    "react-router-test-context": "0.1.0",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "10.0.0",
    "stylelint-config-recommended-scss": "3.2.0",
    "stylelint-scss": "3.5.4",
    "svg-react-loader": "0.4.6",
    "wait-on": "3.2.0",
    "webpack": "3.12.0",
    "webpack-cli": "3.3.1",
    "webpack-dev-server": "2.11.5",
    "webpack-merge": "4.2.1"
  },
  "build": {
    "appId": "io.nos.desktop",
    "forceCodeSigning": true,
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "win": {
      "icon": "./static/icons/icon1024x1024.ico"
    },
    "linux": {
      "icon": "./static/icons/icon1024x1024.png"
    },
    "directories": {
      "buildResources": "static"
    },
    "files": [
      "static/**/*"
    ],
    "publish": {
      "provider": "github"
    }
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "setupFiles": [
      "./__tests__/setupTests.js",
      "jest-canvas-mock"
    ],
    "setupTestFrameworkScriptFile": "./__tests__/setupFramework.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "^.+\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
