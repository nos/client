{
  "name": "nOS",
  "description": "nOS: NEO Operating System",
  "author": "nOS",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "license": "MIT",
  "dependencies": {
    "@cityofzion/neon-js": "3.6.2",
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-regular": "^5.0.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "@fortawesome/react-fontawesome": "^0.0.17",
    "@ledgerhq/hw-transport-node-hid": "^4.7.0",
    "bignumber.js": "^7.0.1",
    "classnames": "^2.2.5",
    "electron-is-dev": "^0.3.0",
    "electron-json-storage": "^4.1.0",
    "es6-promisify": "^6.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "rc-tooltip": "^3.7.2",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-breadcrumbs-hoc": "^2.1.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "spunky": "^1.2.1",
    "uuid": "^3.2.1",
    "what-input": "^5.0.5"
  },
  "scripts": {
    "start": "concurrently -r \"yarn run start-react\" \"wait-on http://localhost:3000/ && yarn run start-electron\" --kill-others --success first",
    "build": "react-app-rewired build",
    "pretest": "yarn run lint",
    "test": "react-app-rewired test --env=jsdom --testMatch \"**/__tests__/**/*.test.{js,jsx,mjs}\" --setupTestFrameworkScriptFile \"./__tests__/setupTests.js\"",
    "start-react": "cross-env BROWSER=none react-app-rewired start",
    "start-electron": "cross-env ELECTRON_START_URL=http://localhost:3000/ electron .",
    "pack": "yarn run build && electron-builder --dir",
    "dist": "yarn run build && cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true electron-builder",
    "lint": "eslint --env browser,node,server --ext .jsx,.js --color .",
    "lint:fix": "yarn run lint --fix",
    "clean": "rimraf build dist",
    "postinstall": "electron-builder install-app-deps && electron-rebuild --force"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "electron": "^2.0.0",
    "electron-builder": "^20.11.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.7.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "postcss-flexbugs-fixes": "^3.3.0",
    "react-app-rewired": "^1.4.1",
    "react-router-test-context": "^0.1.0",
    "react-scripts": "^1.1.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "wait-on": "^2.1.0",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.2"
  },
  "build": {
    "appId": "io.nos.desktop",
    "win": {
      "icon": "./public/icons/icon1024x1024.ico"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "homepage": "./"
}
