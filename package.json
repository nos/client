{
  "name": "nOS",
  "description": "nOS: NEO Operating System",
  "author": "nOS",
  "version": "0.2.2",
  "private": true,
  "main": "dist/main/main.js",
  "license": "MIT",
  "engines": {
    "node": "^8.10.0 || >=9.10.0"
  },
  "dependencies": {
    "@cityofzion/neon-js": "3.9.2",
    "@fortawesome/fontawesome-svg-core": "1.2.2",
    "@fortawesome/free-regular-svg-icons": "5.2.0",
    "@fortawesome/free-solid-svg-icons": "5.2.0",
    "@fortawesome/react-fontawesome": "0.1.0",
    "@ledgerhq/hw-transport-node-hid": "4.18.0",
    "bignumber.js": "7.2.1",
    "classnames": "2.2.6",
    "electron-is-dev": "0.3.0",
    "electron-json-storage": "4.1.1",
    "es6-promisify": "6.0.0",
    "format-currency": "1.0.0",
    "history": "4.7.2",
    "lodash": "4.17.10",
    "mime-types": "2.1.19",
    "node-fetch": "2.2.0",
    "prop-types": "15.6.2",
    "rc-tooltip": "3.7.2",
    "react": "16.4.1",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.6",
    "react-router-dom": "4.3.0",
    "react-router-redux": "next",
    "recharts": "1.1.0",
    "recompose": "0.27.1",
    "redux": "3.7.2",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.3.0",
    "source-map-support": "0.5.6",
    "spunky": "1.2.1",
    "switch-tree": "0.2.2",
    "uuid": "3.3.2",
    "what-input": "5.1.1"
  },
  "scripts": {
    "start": "yarn build:main && run-p -l -n -r start:renderer start:electron",
    "start:renderer": "webpack-dev-server --config config/webpack.renderer.js",
    "start:electron": "wait-on http://localhost:9080/ && electron .",
    "build": "yarn build:main && yarn build:renderer",
    "build:main": "electron-webpack main",
    "build:renderer": "cross-env NODE_ENV=production webpack --config config/webpack.renderer.js",
    "dist": "yarn run clean && yarn run build && cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true electron-builder",
    "pack": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "pretest": "yarn run lint",
    "test": "jest",
    "lint": "eslint --env browser,node,server --ext .jsx,.js --color .",
    "lint:fix": "yarn run lint --fix",
    "clean": "rimraf build dist coverage",
    "postinstall": "electron-builder install-app-deps && electron-rebuild --force"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-plugin-dynamic-import-node": "2.0.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-classes": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-react": "6.24.1",
    "codecov": "3.0.4",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "electron": "2.0.5",
    "electron-builder": "20.26.0",
    "electron-devtools-installer": "2.2.4",
    "electron-rebuild": "1.8.1",
    "electron-webpack": "1.13.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.1.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.4.1",
    "node-loader": "0.6.0",
    "node-sass": "4.9.2",
    "npm-run-all": "4.1.3",
    "react-hot-loader": "4.3.4",
    "react-router-test-context": "0.1.0",
    "redux-devtools-extension": "2.13.5",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "svg-react-loader": "0.4.5",
    "wait-on": "2.1.0",
    "webpack": "3.11.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.3"
  },
  "build": {
    "appId": "io.nos.desktop",
    "win": {
      "icon": "./static/icons/icon1024x1024.ico"
    },
    "directories": {
      "buildResources": "static"
    },
    "files": [
      "static/**/*"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "redux",
      "webpack",
      "webpack-dev-server"
    ]
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "setupFiles": [
      "./__tests__/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "^.+\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
