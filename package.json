{
  "name": "nos",
  "description": "nOS: NEO Operating System",
  "author": "nOS",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "history": "^4.7.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "recompose": "^0.26.0",
    "redux": "^3.7.2"
  },
  "scripts": {
    "start": "concurrently 'yarn run start-react' 'wait-on http://localhost:3000/ && yarn run start-electron' --kill-others --success first",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "start-react": "cross-env BROWSER=none react-app-rewired start",
    "start-electron": "cross-env ELECTRON_START_URL=http://localhost:3000/ electron .",
    "pack": "yarn run build && electron-builder --dir",
    "dist": "yarn run build && electron-builder",
    "clean": "rm -rf build && rm -rf dist",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "electron": "^1.8.2",
    "electron-builder": "^19.56.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.7.2",
    "postcss-flexbugs-fixes": "^3.3.0",
    "react-app-rewired": "^1.4.1",
    "react-scripts": "^1.1.1",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.6",
    "wait-on": "^2.1.0"
  },
  "build": {
    "appId": "com.nos.electron",
    "win": {
      "icon": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "homepage": "./"
}
